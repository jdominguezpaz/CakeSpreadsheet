{
    "name": "jdominguezpaz/cakespreadsheet",
    "type": "cakephp-plugin",
    "description": "CakePHP excel spreadsheet generator using PHPSpreadsheet (CakePHP 3.x compatible, fixed for 3.8+)",
    "keywords": [
        "cakephp",
        "cakephp3",
        "excel",
        "phpspreadsheet",
        "xlsx"
    ],
    "homepage": "https://github.com/jdominguezpaz/CakeSpreadsheet",
    "license": "MIT",
    "authors": [
        {
            "name": "Christakis Mina",
            "email": "christosmina@gmail.com"
        },
        {
            "name": "Jaime DomÃ­nguez Paz",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.6",
        "phpoffice/phpspreadsheet": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeSpreadsheet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeSpreadsheet\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support": {
        "issues": "https://github.com/jdominguezpaz/CakeSpreadsheet/issues",
        "source": "https://github.com/jdominguezpaz/CakeSpreadsheet"
    }
}